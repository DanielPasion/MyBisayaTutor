{
  "expo": {
    "name": "MyBisayaTutor",
    "slug": "MyBisayaTutor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/logo.png",
    "scheme": "mybisayatutor",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.danielpasion.MyBisayaTutor",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./app/assets/images/logo.png",
        "backgroundImage": "./app/assets/images/logo.png",
        "monochromeImage": "./app/assets/images/logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.danielpasion.MyBisayaTutor"
    },
    "web": {
      "output": "static",
      "favicon": "./app/assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./app/assets/images/logo.gif",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#EC6D3B",
          "dark": {
            "backgroundColor": "#EC6D3B"
          }
        }
      ],
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "89adc2b1-c3d4-405b-844f-fc3f4fb5a199"
      }
    }
  }
}
